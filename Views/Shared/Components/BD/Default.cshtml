@{
}
@model IEnumerable<string>

<div class="d-grid gap-1">
@foreach (string bd in Model)
{
   

    <a asp-controller="Home" asp-action="Burials"
   asp-route-hairColor="@ViewBag.SelectedHairColor"
   asp-route-ageAtDeath="@ViewBag.SelectedAgeAtDeath"
   asp-route-burialDepth="@bd"
   asp-route-bSex="@ViewBag.SelectionSex"
   asp-route-pageNum="1"
       class="btn @(ViewBag.SelectedAOD == bd ? "btn-primary" : "btn-outline-secondary")">@bd</a>
    <br />
}
</div>