@{
}
@model IEnumerable<string>

@foreach (string bd in Model)
{
    <a asp-controller="Home" asp-action="Burials" asp-route-burialDepth="@bd" asp-route-pageNum="1"
       class="btn @(ViewBag.SelectedBD == bd ? "btn-primary" : "btn-outline-secondary")">@bd</a>
    <br />
}