@{
}
@model IEnumerable<string>

@foreach (string aod in Model)
{
    <a asp-controller="Home" asp-action="Burials" asp-route-ageAtDeath="@aod" asp-route-pageNum="1"
    class="btn @(ViewBag.SelectedAOD == aod ? "btn-primary" : "btn-outline-secondary")">@aod</a>
    <br />
}