@{
}
@model INTEX.Models.burialmain

<h2 class="text-center text-dark-emphasis">BYU Archaeology's Predictive Modeling</h2>
<div>
    <video width=“560” height=“315" src=“https://youtu.be/Hn3OuyhXcJ4” title=“YouTube video player” frameborder=“0” allow=“accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share” allowfullscreen></video>
</div>
<h2 class="mb-4">Enter the data to predict burial direction/orientation</h2>
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <h3>Mummy Information</h3>
            <div class="form-group">
                <label>Sex:</label>
                <input asp-for="sex" class="form-control" />
            </div>
            <div class="form-group">
                <label>Hair:</label>
                <input asp-for="hair" class="form-control" />
            </div>
            <div class="form-group">
                <label>Hair Color:</label>
                <input asp-for="haircolor" class="form-control" />
            </div>
            <div class="form-group">
                <label>Age at Death:</label>
                <input asp-for="ageatdeath" class="form-control" />
            </div>

            <div class="form-group">
                <label>Adult/Subadult:</label>
                <input asp-for="adultsubadult" class="form-control" />
             </div>
        </form>
     </div>
</div>